# mir_passes

@preserve
"test:main" {}

@preserve
"fold:if_cond_assign" {
	let a: score = val 0s;
	if bio ~0 ~0 ~0, "forest":
		set %a, 1s;
	use %a;
	let b: score = val 1s;
	if pred "foo":
		set %b, 0s;
	use %b;
}

@preserve
"fold:assign_const_add" {
	let a: score = val sco @s[] "foo";
	let b: score = val 0s;
	add %b, %a;
	use %b;

	let c: score = val sco @s[] "foo";
	let d: score = val 10s;
	add %d, %c;
	use %d;
}

@preserve
"fold:overwrite_op" {
	let a: score = val sco @s[] "foo";
	mul %a, 7s;
	set %a, 6s;
	use %a;
}
